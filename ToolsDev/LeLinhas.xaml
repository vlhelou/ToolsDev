<Window x:Class="ToolsDev.LeLinhas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LeLinhas" Height="407" Width="794">
	<Grid>
		<Label Content="Arquivo:" HorizontalAlignment="Left" Margin="13,12,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="txtArquivo" HorizontalAlignment="Left" Height="23" Margin="83,15,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="475"/>
		<Button Content="Lê Linhas" HorizontalAlignment="Left" Margin="588,18,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
		<TextBox x:Name="linhas" TextWrapping="NoWrap" HorizontalAlignment="Left" Height="290" Margin="20,72,0,0"  Text="" VerticalAlignment="Top" Width="655" AcceptsReturn="True"/>
		<Label Content="Linha Inicial:" HorizontalAlignment="Left" Margin="19,45,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="linhaInicial" HorizontalAlignment="Left" Height="23" Margin="100,45,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
		<Label Content="Quantidade de Linhas:" HorizontalAlignment="Left" Margin="280,45,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="qtLinhas" HorizontalAlignment="Left" Height="23" Margin="440,45,0,0" TextWrapping="Wrap" Text="20" VerticalAlignment="Top" Width="120"/>

	</Grid>
</Window>
