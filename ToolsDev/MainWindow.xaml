<Window x:Class="ToolsDev.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="Tools Dev v.3" Height="100" Width="525"
		Icon="ferramenta.ico">
	<Window.TaskbarItemInfo>
		<TaskbarItemInfo></TaskbarItemInfo>
	</Window.TaskbarItemInfo>
	<DockPanel Background="White">
		<Menu DockPanel.Dock="Top" Background="White">
			<MenuItem Header="_Banco">
				<MenuItem Header="Sql2Codigo" Click="clickSql2Codigo" />
				<MenuItem Header="Busca Banco" Click="ClickBuscaBanco" />
				<MenuItem Header="Dicionario" Click="ClickDicionario" />
				<MenuItem Header="Monta IN" Click="ClickMontaIN" />
				<MenuItem Header="Gera Insert" Click="ClickGeraInsert" />
			</MenuItem>
			<MenuItem Header="_Programação">
				<MenuItem Header="Cria Classe" Click="clickCriaClasse" />
				<MenuItem Header="Branch" Click="ClickBranch" />
			</MenuItem>
			<MenuItem Header="_Arquivos">
				<MenuItem Header="Le Linhas" Click="ClickLeLinhas" />
			</MenuItem>

		</Menu>
	</DockPanel>
</Window>
